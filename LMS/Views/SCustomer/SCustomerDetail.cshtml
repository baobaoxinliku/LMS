@model LMS.Models.SCustomer
@{
    Layout = "~/Views/Shared/MyLayout.cshtml";
}
@section TitleSection{
    FCustomerDetail
}
@section HeaderSection{

}
@section ContentBody{
    <div id="content">
        <div id="content-header">
            <div id="breadcrumb">
                <a href="#" title="返回首页" class="tip-bottom"><i class="icon-home"></i>首页</a>
                <a href="#">客户情况</a>
                <a href="#">收货人信息</a>
                <a href="#" class="current">详情</a>
            </div>
            <h1>用户信息</h1><hr />
        </div>
        <div class="body-content container">
            <dl>
                <dt>收货人编号：</dt>
                <dd>@Html.DisplayFor(model => model.SCustoID)</dd>

                <dt><br />收货人姓名：</dt>
                <dd>@Html.DisplayFor(model => model.SCustoName)</dd>

                <dt><br />收货人电话：</dt>
                <dd>@Html.DisplayFor(model => model.SCustoTel)</dd>
            </dl>
            <div>
                @Html.ActionLink("返回首页", "SCustomerList")
            </div>
        </div>
    </div>
}

